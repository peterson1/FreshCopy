<UserControl x:Class="FreshCopy.Client.Lib45.BroadcastHandlers.AppendOnlyDbBroadcastHandlerUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FreshCopy.Client.Lib45.BroadcastHandlers"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel>
        <Label Content="{Binding FileKey}" 
               DockPanel.Dock="Top"
               />
        <ContentControl Content="{Binding Logs}" />
    </DockPanel>
</UserControl>
